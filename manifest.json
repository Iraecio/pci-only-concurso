{
  "name": "Pci Only Concursos",
  "version": "1.0",
  "description": "Extens√£o para localizar concursos no site Pci Concursos",
  "manifest_version": 2,
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://www.pciconcursos.com.br/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.pciconcursos.com.br/concursos/*"],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "default_title": "Pci Only Concursos",
    "default_icon": "icon-128.png"
  }
}
